{
  "branches": [
    "main",
    "beta",
    "next"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    ["@semantic-release/npm", { "pkgRoot": "dist/helpers" }],
    "@semantic-release/git",
    ["@semantic-release/github", { "assets": ["dist/helpers/**"] }]
  ],
  "repositoryUrl": "https://github.com/wanoo21/ngx-intl-helper",
  "tagFormat": "v${version}",
  "preset": "angular"
}
